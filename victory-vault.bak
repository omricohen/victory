    <section class="winning-process">
        <div class="auto-container">
            <h2>How Winning Works</h2>
            <div class="winning-steps">
                <div class="step-card">
                    <div class="step-number">1</div>
                    <div class="step-content">
                        <h3>Stake Your Tokens</h3>
                        <div class="step-visual">
