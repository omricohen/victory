                                    <div class="orbit-item"><i class="fas fa-trophy"></i></div>
                                </div>
                                <div class="orbit orbit-3">
                                    <div class="orbit-item"><i class="fas fa-ticket-alt"></i></div>
                                </div>
                            </div>
                            <div class="connection-lines"></div>
                        </div>
                    </div>
