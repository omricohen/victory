    <section class="rewards-calculator">
        <div class="auto-container">
            <h2>Calculate Your Potential Rewards</h2>
            <div class="calculator-container">
                <div class="calculator-grid">
                    <div class="input-section">
                        <div class="input-group">
                            <label>Staking Amount (VICTRY)</label>
                            <input type="number" id="stakeAmount" placeholder="Enter amount" min="0">
                        </div>
                        <div class="input-group">
                            <label>Staking Period (Days)</label>
                            <input type="number" id="stakePeriod" placeholder="Enter days" min="1" max="365">
                        </div>
                    </div>
                    
                    <div class="results-section">
                        <div class="results">
                            <div class="result-item">
                                <span class="label">Base APY</span>
                                <span class="value" id="baseApy">12%</span>
                            </div>
                            <div class="result-item">
                                <span class="label">Daily Rewards</span>
                                <span class="value" id="dailyRewards">0 VICTRY</span>
                            </div>
                            <div class="result-item">
                                <span class="label">Total Rewards</span>
                                <span class="value" id="totalRewards">0 VICTRY</span>
                            </div>
                            <div class="result-item highlight">
                                <span class="label">Daily Win Chance</span>
                                <span class="value" id="winChance">0%</span>
                            </div>
                        </div>
                    </div>
                </div>
                
                <div class="calculator-note">
                    <i class="fas fa-info-circle"></i>
                    <p>The more you stake, the higher your chances of winning massive rewards. Maximum win chance is capped at 15%.</p>
                </div>
            </div>
        </div>
    </section>

